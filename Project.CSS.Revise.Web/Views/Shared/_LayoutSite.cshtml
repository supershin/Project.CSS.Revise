    <style>
        /* Force sidebar on top of everything */
        .sidebar-wrapper {
            position: relative; /* ensure it can take z-index */
            z-index: 999 !important;
        }

        #filterToggleIcon {
            transition: transform 0.2s ease-in-out;
        }

        #filterToggleIcon.collapsed {
            transform: rotate(0deg);
        }

        #filterToggleIcon.expanded {
            transform: rotate(180deg);
        }

        .position-relative .show-hide {
            font-size: 0.9rem;
            user-select: none;
        }

        .clickable-card {
            cursor: pointer;
            transition: transform 0.2s ease, box-shadow 0.2s ease;
        }

        .clickable-card:hover {
            transform: translateY(-5px);
            box-shadow: 0 4px 10px rgba(0, 0, 0, 0.2);
        }

        .clickable-card.active {
            background-color: #f0f8ff;
            border: 2px solid #007bff;
        }

        .scroll-demo {
            scroll-behavior: smooth;
        }

        .badge-small {
            font-size: 0.65rem;
            padding: 0.25em 0.45em;
        }

        .hover-icon-swap {
            display: flex;
            align-items: center;
            justify-content: center;
            height: 80px;
            width: 80px;
            margin: auto;
            background-color: #f8f9fa;
            border-radius: 50%;
            transition: background-color 0.3s ease;
            cursor: pointer;
        }

        .hover-icon-swap:hover {
            background-color: #e9ecef;
        }

        .hover-icon-swap i {
            font-size: 40px;
            color: #6c757d;
            transition: transform 0.3s ease, color 0.3s ease;
        }

        .text-scroll-horizontal {
            display: inline-block;
            white-space: nowrap;
            overflow-x: auto;
            max-width: 160px; /* Adjust based on your layout */
            vertical-align: middle;
        }

        .choices__input {
            font-size: 0.9rem;
        }


    </style>
