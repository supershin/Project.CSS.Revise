                                <tr>
                                    <th class="f-light" scope="col">Month</th>
                                    <th class="f-light text-end" scope="col">Target</th>
                                    <th class="f-light text-end" scope="col">Rolling</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td class="f-w-400 f-10"> <a class="line-clamp" href="cart.html">Jan</a></td>
                                    <td>
                                        <div class="progress-value d-flex gap-2 align-items-center">
                                            <div class="progress">
                                                <div class="progress-bar bg-primary" role="progressbar" style="width: 75%  " aria-valuenow="75" aria-valuemin="0" aria-valuemax="100"></div>
                                            </div><span>12M</span>
                                        </div>
                                    </td>
                                    <td>
                                        <div class="progress-value d-flex gap-2 align-items-center">
                                            <div class="progress">
                                                <div class="progress-bar bg-primary" role="progressbar" style="width: 75%  " aria-valuenow="75" aria-valuemin="0" aria-valuemax="100"></div>
                                            </div><span>15M</span>
                                        </div>
                                    </td>
                                </tr>
                                <tr>
                                    <td class="f-w-400 f-10"> <a class="line-clamp" href="cart.html">Feb</a></td>
                                    <td>
                                        <div class="progress-value d-flex gap-2 align-items-center">
                                            <div class="progress">
                                                <div class="progress-bar bg-primary" role="progressbar" style="width: 75%  " aria-valuenow="75" aria-valuemin="0" aria-valuemax="100"></div>
                                            </div><span>12M</span>
                                        </div>
                                    </td>
                                    <td>
                                        <div class="progress-value d-flex gap-2 align-items-center">
                                            <div class="progress">
                                                <div class="progress-bar bg-primary" role="progressbar" style="width: 75%  " aria-valuenow="75" aria-valuemin="0" aria-valuemax="100"></div>
                                            </div><span>15M</span>
                                        </div>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>