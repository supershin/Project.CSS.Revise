<script>
  document.addEventListener("DOMContentLoaded", function () {
          new Choices('#projectMultiSelect', {
            removeItemButton: true,
              placeholderValue: 'เลือกโครงการได้หลายโครงการ',
            searchEnabled: true,
            itemSelectText: '',
            shouldSort: false
        });
    });

      document.addEventListener("DOMContentLoaded", function () {
          document.querySelectorAll('.bank-dropdown').forEach(function (element) {
              new Choices(element, {
                  allowHTML: true,
                  itemSelectText: '',
                  searchEnabled: true,
                  shouldSort: false,
                  callbackOnCreateTemplates: function (template) {
                      return {
                          item: (classNames, data) => {
                              const img = data.customProperties?.image || '';
                              return template(`
                                  <div class="${classNames.item} ${data.highlighted ? classNames.highlightedState : classNames.itemSelectable}"
                                       data-item data-id="${data.id}" data-value="${data.value}" ${data.active ? 'aria-selected="true"' : ''}>
                                      ${img ? `<img src="${img}" width="20" class="me-2" style="vertical-align:middle;">` : ''}
                                      ${data.label}
                                  </div>
                              `);
                          },
                          choice: (classNames, data) => {
                              const img = data.customProperties?.image || '';
                              return template(`
                                  <div class="${classNames.item} ${classNames.itemChoice} ${data.disabled ? classNames.itemDisabled : classNames.itemSelectable}"
                                       data-select-text="${this.config.itemSelectText}" data-choice
                                       data-id="${data.id}" data-value="${data.value}" ${data.disabled ? 'data-choice-disabled aria-disabled="true"' : 'data-choice-selectable'}
                                       ${data.selected ? 'data-choice-selected' : ''}>
                                      ${img ? `<img src="${img}" width="20" class="me-2" style="vertical-align:middle;">` : ''}
                                      ${data.label}
                                  </div>
                              `);
                          }
                      };
                  }
              });
          });
      });
</script>


